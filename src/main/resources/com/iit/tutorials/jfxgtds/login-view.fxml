<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iit.tutorials.jfxgtds.LoginController">
   <children>
      <Label layoutX="58.0" layoutY="124.0" prefHeight="28.0" prefWidth="75.0" text="Username">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="loginLabal" alignment="CENTER" layoutX="82.0" layoutY="30.0" prefHeight="55.0" prefWidth="434.0" text="Login to Government Tax System" textFill="#0717ac" textOverrun="CLIP">
         <font>
            <Font name="Bell MT Bold" size="27.0" />
         </font>
      </Label>
      <Label layoutX="58.0" layoutY="177.0" prefHeight="28.0" prefWidth="75.0" text="Password">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="passInput" layoutX="169.0" layoutY="178.0" onAction="#onLoginClick" prefHeight="25.0" prefWidth="198.0" />
      <TextField fx:id="useNameText" layoutX="169.0" layoutY="126.0" prefHeight="25.0" prefWidth="198.0" />
      <Button layoutX="238.0" layoutY="231.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="36.0" prefWidth="59.0" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" layoutX="17.0" layoutY="301.0" prefHeight="55.0" prefWidth="560.0" textFill="#bc0606">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
