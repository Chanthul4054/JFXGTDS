<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iit.tutorials.jfxgtds.LoginController">
   <Label alignment="CENTER" layoutX="57.0" layoutY="120.0" prefHeight="36.0" prefWidth="99.0" text="Username">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
   </Label>
   <Label fx:id="loginLabal" alignment="CENTER" layoutX="55.0" layoutY="31.0" prefHeight="55.0" prefWidth="490.0" text="Login to Government Tax System" textFill="#0717ac" textOverrun="CLIP">
      <font>
         <Font name="Bell MT Bold" size="30.0" />
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="57.0" layoutY="182.0" prefHeight="36.0" prefWidth="99.0" text="Password">
      <font>
         <Font name="System Bold" size="20.0" />
      </font>
   </Label>
   <PasswordField fx:id="passInput" layoutX="208.0" layoutY="182.0" onAction="#onLoginClick" prefHeight="36.0" prefWidth="209.0">
      <font>
         <Font size="20.0" />
      </font></PasswordField>
   <TextField fx:id="useNameText" layoutX="208.0" layoutY="120.0" prefHeight="36.0" prefWidth="209.0">
      <font>
         <Font size="20.0" />
      </font></TextField>
   <Button cache="true" depthTest="DISABLE" layoutX="235.0" layoutY="258.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="47.0" prefWidth="123.0" text="Login">
      <font>
         <Font name="System Bold" size="22.0" />
      </font>
   </Button>
   <Label fx:id="messageLabel" layoutX="17.0" layoutY="330.0" prefHeight="55.0" prefWidth="560.0" textFill="#bc0606">
      <font>
         <Font name="System Bold" size="31.0" />
      </font>
   </Label>
</AnchorPane>
